{
  "kind": "collectionType",
  "collectionName": "articulos",
  "info": {
    "singularName": "articulo",
    "pluralName": "articulos",
    "displayName": "articulos"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "descripcion": {
      "type": "richtext"
    },
    "tag": {
      "type": "string"
    },
    "fecha": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo"
    },
    "autor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "articulos"
    },
    "imagen": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
