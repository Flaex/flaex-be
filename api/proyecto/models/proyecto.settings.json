{
  "kind": "collectionType",
  "collectionName": "proyectos",
  "info": {
    "name": "proyecto"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "descripcion": {
      "type": "richtext"
    },
    "ciudad": {
      "type": "string"
    },
    "miniatura": {
      "type": "component",
      "repeatable": false,
      "component": "informacion.imagen"
    },
    "imagenes": {
      "type": "component",
      "repeatable": true,
      "component": "informacion.imagen"
    },
    "categoria": {
      "type": "enumeration",
      "enum": [
        "logotipos",
        "impresos",
        "web",
        "tipografia"
      ]
    },
    "meta": {
      "type": "component",
      "repeatable": true,
      "component": "informacion.meta"
    }
  }
}
